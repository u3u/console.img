{
  "name": "console.img",
  "version": "3.0.2",
  "description": "âœ¨ Output image in browser console.",
  "keywords": [
    "console",
    "console.img",
    "console.image"
  ],
  "repository": {
    "url": "https://github.com/u3u/console.img.git"
  },
  "license": "MIT",
  "author": "u3u",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "jsdelivr": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "module": "./dist/index.js",
  "browser": "./dist/index.global.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm,iife --dts-resolve --no-splitting --shims --clean",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm",
    "prepublishOnly": "pnpm build"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,md,json}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@types/node": "^18.15.13",
    "@u3u/prettier-config": "^1.0.2",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "packageManager": "pnpm@8.3.1",
